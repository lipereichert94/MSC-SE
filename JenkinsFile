pipeline {
    agent any
    tools {
        maven 'mvn 3.9.7'
    }
    stages {
        stage('Build') { 
            steps {
                    git url: 'https://github.com/cyrille-leclerc/multi-module-maven-project'
                sh 'mvn -B -DskipTests clean package'
                archive 'target/*.jar' 
            
            }
        }
    }
}

