pipeline {
    agent any
    tools {
        maven 'Maven 3.3.9'
    }
    stages {
        stage('Build') { 
            steps {
                    git url: 'https://github.com/cyrille-leclerc/multi-module-maven-project'
                            withMaven {
                sh 'mvn -B -DskipTests clean package' 
            }
            }
        }
    }
}

